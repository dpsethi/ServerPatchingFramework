        <script><![CDATA[var CiList = '' + producer.select_cis;
var ciArray = CiList.split(",");
for (var i = 0; i < ciArray.length; i++) {
gs.info("DPS found host in record producer " + ciArray[i]);
	var gr = new GlideRecord('cmdb_ci_server');
	gr.addQuery('sys_id',ciArray[i]);
	gr.query();
	gr.next();
	//gr.u_os_patching_team = "bb5921074fde3ac02f5aa34f9310c709";
	gr.u_os_patching_team = producer.os_patching_team;
	gr.u_patch_day_of_month = producer.patch_day_of_month;
	gr.u_patch_start_time = producer.patch_start_time;
	gr.u_patch_data_source = "Manual";
	gr.update();

}
gs.addInfoMessage("Patching request submitted");
//gs.setRedirect(current);]]></script>
